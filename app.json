{
  "name": "experiments-viewer",
  "scripts": {
  },
  "env": {
    "NODE_ENV": "production",
    "SESSION_COOKIE_SECURE": "True",
    "SSL_REDIRECT": "True",
    "DATABASE_URL": {
      "required": true
    },
    "GOOGLE_AUTH_KEY": {
      "required": true
    },
    "GOOGLE_AUTH_SECRET": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ]
}
